  "devDependencies": {
    "@vue/eslint-config-prettier": "^6.0.0",
    "axios": "^0.21.1",
    "babel-plugin-add-module-exports": "^1.0.2",
    "eslint": "^8.39.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^8.7.1",
    "prettier": "^1.19.1",
    "vite": "^4.3.2",
